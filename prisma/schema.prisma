generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cake {
  id Int @id @default(autoincrement())
  name String
  pictureURL String?
  bakerName String
  reviews Review[]
}

model Review{
  id String @id @default(cuid())
  rating Int
  cake Cake @relation(references: [id],fields: [cakeId],onDelete:Cascade)
  cakeId Int 
}